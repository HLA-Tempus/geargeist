{
    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm run dev",
            "name": "Run npm run dev",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Launch browser",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}/src",
            "runtimeExecutable": "/Applications/Chromium.app/Contents/MacOS/Chromium"
        }
    ]
}
